{
  "name": "rinkeby-migrations-tests",
  "version": "0.0.1",
  "description": "Experiment migrating PLCRVoting contracts to Testnets w/ Truffle V5",
  "scripts": {
    "build": "cd truffle/packages/truffle && npm run build",
    "bootstrap": "./bootstrap.sh",
    "install": "./node_modules/.bin/truffle install",
    "compile": "./node_modules/.bin/truffle compile",
    "test": "./node_modules/.bin/truffle test",
    "migrate:dev": "/.node_modules/.bin/truffle migrate --network development",
    "migrate:rinkeby": "./node_modules/.bin/truffle migrate --network rinkeby",
    "migrate:dry": "./node_modules/.bin/truffle migrate --dry-run"
  },
  "author": "Yorke Rhodes <yorke.rhodesiv@consensys.net>, Aspyn Palatnick <aspyn.palatnick@consensys.net>, Cem Ozer <cem.ozer@consensys.net>",
  "license": "ISC",
  "dependencies": {
    "truffle-hdwallet-provider": "0.0.5"
  }
}
